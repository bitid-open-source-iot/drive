{
    "limit": 50,
    "production": false,
    "authentication": false,
    "auth": {
        "host": "http://127.0.0.1:9000",
        "token": "xxx",
        "email": "xxx@xxx.co.za",
        "appId": "xxx"
    },
    "logger": {
        "appenders": {
            "drive": {
                "type": "file",
                "backups": 10,
                "filename": "./logs/drive.log",
                "absolute": true,
                "maxLogSize": 102400
